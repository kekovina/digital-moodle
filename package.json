{
  "name": "confusionserver",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "server": "nodemon ./bin/www",
    "client": "npm run start --prefix client",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "devc": "npm run client",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "express-session": "^1.17.1",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.4",
    "mongoose-currency": "^0.2.0",
    "morgan": "~1.9.0",
    "multer": "^1.4.2",
    "node-telegram-bot-api": "^0.52.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.1",
    "rc-field-form": "^1.20.0",
    "react-sticky": "^6.0.3",
    "session-file-store": "^1.5.0",
    "socket.io": "^2.4.1"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.7"
  }
}
